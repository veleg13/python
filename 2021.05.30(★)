def 크기가_1인_사각형_가로_출력(n):
             #반복
             for i in range(n):                         
                          #사각형 출력하기
                          print('@',end='')
             #다음줄로 커서이동
             print()


def 넓이에_맞는_사각형_출력(a, b):
             #반복
             for i in range(a):
                          크기가_1인_사각형_가로_출력(b)
             print();print();print()

r=int(input('넓이를 입력해주세요:'))

for i in range(1,r+1):
             if r%i==0 and i*i <= r: #약수찾기
                          넓이에_맞는_사각형_출력(i,r//i)
